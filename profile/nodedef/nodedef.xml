<nodeDefs>
  <nodeDef id="husqvarna" nls="ctl">
    <editors />
    <sts>
      <st id="ST" editor="bool" />
    </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="DISCOVER" />
        <cmd id="REMOVE_NOTICES_ALL" />
        <cmd id="UPDATE_PROFILE" />
      </accepts>
    </cmds>
  </nodeDef>

  <nodeDef id="mower" nls="MOW">
    <sts>
      <st id="ST" editor="bool" />
      <st id="GV0" editor="percent" />
      <st id="GV1" editor="mode" />
      <st id="GV2" editor="error" />
      <st id="GV3" editor="start_source" />
      <st id="GV4" editor="start_timestamp" />
      <st id="GV5" editor="st_mode" />
      <st id="GV6" editor="st_activity" />
      <st id="GV7" editor="st_reason" />
      <st id="GV8" editor="st_state" />
      <st id="GV9" editor="st_type" />
    </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="START">
          <p id="override" editor="start_time" />
        </cmd>
        <cmd id="PAUSE" />
        <cmd id="PARK" />
        <cmd id="STOP" />
      </accepts>
    </cmds>
  </nodeDef>

</nodeDefs>
